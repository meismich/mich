use assets;

select distinct(xassets.id), xassets.s_location, xassets.s_devicetype, interog8s.s_interog8
	from xassets left join interog8s
		on xassets.id = interog8s.i_asset
	where xassets.s_status = "Active"
		and (xassets.id < 90000000 or xassets.id > 200700000)
		and xassets.s_devicetype in ("PC", "Notebook", "Zbox", "Tablet PC")
		and interog8s.i_i8type = 2
	;
